  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start-backend": "./app",
    "build-backend": "go build ../backend/app.go",
    "start-modules": "npm run delay && npm-run-all -p start-module-main start-module-hue-control",
    "start-module-main": "./main http://localhost:3000",
    "start-module-hue-control": "./hue-control http://localhost:3000",
    "build-modules": "go build ../../../module-library/example/main.go && go build ../../../module-library/hue-control/hue-control.go",
    "start-all": "npm run build-backend && npm run build-modules && npm-run-all -p  start-backend start-modules start",
    "delay": "node ./node_modules/npm-delay 2000",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
